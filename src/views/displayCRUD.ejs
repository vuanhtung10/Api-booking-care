<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>get CRUD</title>
</head>
<style>
    table,
    th,
    td {
        border: 1px solid black;
        border-collapse: collapse;
    }
    a.btn-edit {
    outline: none;
    border: none;
    height: 25px;
    background: yellow;
    cursor: pointer;
    text-decoration: none;
    }

    a.btn-delete {
    outline: none;
    border: none;
    height: 25px;
    background: red;
    cursor: pointer;
    text-decoration: none;
    }
    </style>
<body>
    <table style="width:100%;">
        <tr>
          <th>Email</th>
          <th>Firstname</th>
          <th>Lastname</th>
          <th>address</th>
          <th>action</th>
        </tr>
        <% for(let i=0; i < dataTable.length; i++) { %>
            <tr>
              <td><%= dataTable[i].email %></td>
              <td><%= dataTable[i].firstName %></td>
              <td><%= dataTable[i].lastName %></td>
              <td><%= dataTable[i].address %></td>
              <td>
                <a href="/edit-crud?id=<%=dataTable[i].id %>" class="btn-edit" type="button">Edit</a>
                <a href="/delete-crud?id=<%=dataTable[i].id %>" class="btn-delete" type="button">Delete</a>
              </td>
            </tr>
         <% } %>
      </table>
</body>
</html>